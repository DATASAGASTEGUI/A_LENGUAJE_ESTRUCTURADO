Algoritmo Ejercicio_0071
	c = AZAR(4)
	Si c = 0 Entonces
		x <- 3
	FinSi
	Si c = 1 Entonces
		x <- 5
	FinSi
	Si c = 2 Entonces
		x <- 7
	FinSi
	Si c = 3 Entonces
		x <- 9
	FinSi
	
	Si x = 3 Entonces
		Escribir x," = Gama Baja"
	FinSi
	Si x = 5 Entonces
		Escribir x," = Gama Media"
	FinSi
	Si x = 7 Entonces
		Escribir x," = Gama Alta"
	FinSi
	Si x = 9 Entonces
		Escribir x," = Entusiasta"
	FinSi
	
FinAlgoritmo
