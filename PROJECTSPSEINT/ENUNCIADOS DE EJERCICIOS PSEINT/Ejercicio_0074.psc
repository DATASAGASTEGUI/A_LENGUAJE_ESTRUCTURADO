Algoritmo Ejercicio_0074
	Definir matriz Como Entero
	nf <- 2
	nc <- 3
	Dimension matriz(nf,nc)
	//Llenar matriz  con números enteros
	matriz(1,1) = 1; matriz(1,2) = 2; matriz(1,3) = 3
	matriz(2,1) = 4; matriz(2,2) = 5; matriz(2,3) = 6
	//Mostrar matriz
	Escribir Sin Saltar matriz(1,1) "  "
	Escribir Sin Saltar matriz(1,2) "  " 
	Escribir matriz(1,3)
	Escribir Sin Saltar matriz(2,1) "  "
	Escribir Sin Saltar matriz(2,2) "  "
	Escribir  matriz(2,3)
FinAlgoritmo
