Algoritmo Ejercicio_0070
	c = AZAR(5)
	Si c = 0 Entonces
		c <- 3
	FinSi
	Si c = 1 Entonces
		c <- 5
	FinSi
	Si c = 2 Entonces
		c <- 7
	FinSi
	Si c = 4 Entonces
		c <- 9
	FinSi
	
	Si c = 3 Entonces
		Escribir c," = Gama Baja"
	FinSi
	Si c = 5 Entonces
		Escribir c," = Gama Media"
	FinSi
	Si c = 7 Entonces
		Escribir c," = Gama Alta"
	FinSi
	Si c = 9 Entonces
		Escribir c," = Entusiasta"
	FinSi
	
FinAlgoritmo
