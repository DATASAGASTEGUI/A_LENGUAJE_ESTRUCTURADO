Algoritmo Switch1
	//Escribir Sin Saltar "pulse enter para generar un día aleatorio ";
	n = Azar(7) + 1
	Segun n Hacer
		1:
			Escribir "es lunes";
		2:
			Escribir "es martes";
		3:
			Escribir "es miércoles";
		4:
			Escribir "es jueves";
		5:
			Escribir "es viernes";
		6:
			Escribir "es sábado"
		7:
			Escribir "es domingo"
	Fin Segun
	Escribir "FIN"
FinAlgoritmo

